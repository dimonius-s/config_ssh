Host *.com
        StrictHostKeyChecking no
        User $User
        ForwardAgent yes
        IdentityFile /home/$User/.ssh/id_rsa
        IdentitiesOnly yes
        UserKnownHostsFile=/dev/null
        #UseKeychain yes
        AddKeysToAgent yes
        ServerAliveInterval 60
        ServerAliveCountMax 1200
Host host
        StrictHostKeyChecking no
        User $User
        ForwardAgent yes
        IdentityFile /home/$User/.ssh/id_rsa_second
        IdentitiesOnly yes
        UserKnownHostsFile=/dev/null
        #UseKeychain yes
        AddKeysToAgent yes
        ServerAliveInterval 60
        ServerAliveCountMax 1200
